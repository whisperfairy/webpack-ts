{
    "compilerOptions": {
        "outDir": "./built/",
        "sourceMap": true,
        "moduleResolution": "Node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": false,
        "target": "es6",
        "jsx": "react",
        "allowSyntheticDefaultImports":true,
        "allowJs": true
    },

    "include": [
        "./src/**/*"

    ],
    "exclude": [
        "node_modules",
        "./src/Server/*"
    ]
}